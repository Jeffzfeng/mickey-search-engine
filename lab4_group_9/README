import paramiko
# paramiko setup 
key = paramiko.RSAKey.from_private_key_file('new_key_final.pem') 
con = paramiko.SSHClient()
con.set_missing_host_key_policy(paramiko.AutoAddPolicy())
con.connect( hostname = "13.59.200.67", username = "ec2-user", pkey = key)
#con.connect( hostname = "ec2-%s.compute-1.amazonaws.com", username = "ec2-user", pkey = key % new_instance.ip_address.encode('utf-8'))
commands = ["echo hi"]
for command in commands:
    print "Executing {}".format(command)
    stdin , stdout, stderr = con.exec_command(command) # this command is executed on the *remote* server
    print stdout.read()
    print( "Errors")
    print stderr.read()


con.close()